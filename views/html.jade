doctype html
html
	head
		meta(http-equiv='Content-Type', content='text/html;charset=utf-8')
		meta(http-equiv='X-UA-Compatible',content='IE=edge,chrome=1')
		meta(name='viewport', content='width=device-width, initial-scale=1')

		block seo
		if headTitle
			title= config.name + ' - ' + headTitle
		block og
			meta(property='og:site_name', content=config.name)

		block head
			link(rel='stylesheet', type='text/css', href='/static/libs/bootstrap-3.3/css/bootstrap.min.css')
			//- link(rel='stylesheet', type='text/css', href='/static/libs/bootstrap-3.3/css/bootstrap-rtl.min.css')
			//- link(rel='stylesheet', type='text/css', href='//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/themes/smoothness/jquery-ui.css')

			//if lt IE 9
				script(src='//oss.maxcdn.com/libs/bootstrap-3.3/js/html5shiv.js')
				script(src='//oss.maxcdn.com/libs/bootstrap-3.3/js/respond.min.js')

	body(class=bodyClass, data-spy=bodySpy, data-target=bodyTarget)
		block body

		block scripts
			script(src='//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js', type='text/javascript')
			//- script(src='//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js', type='text/javascript')
			script(type='text/javascript', src='/static/libs/bootstrap-3.3/js/bootstrap.min.js')

		| <script type="text/javascript">
		block script
		| </script>

		block end